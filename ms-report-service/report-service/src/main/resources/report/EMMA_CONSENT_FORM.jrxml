	<queryString>
		<![CDATA[SELECT 	B.ID,
	COALESCE(B.CUSTOMER_NAME,'') CUSTOMER_NAME,
	Q.IS_USED_SERVICE,
	COALESCE(REPLACE(REPLACE(REPLACE(Q.BRANCH, '01', 'แพรกษา'),'02','อโศก'),'03','พระรามสอง'),'') AS BRANCH ,
	COALESCE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(CONECTION_CHANELS,'01','เฟสบุค'),'02','อินสตาเเกรม'),'03','ไลน์'),'04','คลินิก'),'05','โทรศัพท์'),'') AS CONECTION_CHANELS,
	COALESCE(CONCAT(CONCAT((CASE WHEN IS_FIX_SURGERY THEN 'แก้ไข' WHEN IS_NEW_SURGERY THEN 'เสริมใหม่' END),
	REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(REPLACE(INTEREST_SERVICE,'01','โบท็อก'),'02','ฟิลเลอร์'),'03','เลเซอร์'),'04','ฉีดผิว'),'05','จมูก'),'06','คาง')),CASE WHEN OTHER_INTEREST IS NOT NULL THEN CONCAT(',',OTHER_INTEREST) ELSE '' END),'') AS INTEREST_SERVICE,
	COALESCE(REPLACE(REPLACE(REPLACE(COUNT_ALL_SURGERY,'01','1 ครั้ง'),'02','2 ครั้ง'),'03','มากกว่า 3 ครั้ง'),'') AS COUNT_ALL_SURGERY,
	COALESCE(REPLACE(REPLACE(REPLACE(LATEST_SURGERY_TIME,'01','น้อยกว่า 1 เดือน'),'02','1-3 เดือน'),'03','นานกว่า 3 เดือน'),'') AS LATEST_SURGERY_TIME,
	COALESCE(CONGENITAL_DISEASE,'') AS CONGENITAL_DISEASE,
	DRUG_NAME,
        COALESCE(REPLACE(REPLACE(REPLACE(STOP_TAKE_DRUG_TIME,'01','น้อยกว่า 1 เดือน'),'02','1-3 เดือน'),'03','นานกว่า 3 เดือน'),'') AS STOP_TAKE_DRUG_TIME,
	CASE WHEN CERTAIN_DISEASES LIKE '%01%' THEN TRUE ELSE FALSE END AS IS_DEPRESSION,
	CASE WHEN CERTAIN_DISEASES LIKE '%02%' THEN TRUE ELSE FALSE END AS IS_DIABETES,
	CASE WHEN CERTAIN_DISEASES LIKE '%03%' THEN TRUE ELSE FALSE END AS IS_HEART_DISEASE,
	CASE WHEN CERTAIN_DISEASES LIKE '%04%' THEN TRUE ELSE FALSE END AS IS_HEPATISTIC_B,
	CASE WHEN CERTAIN_DISEASES LIKE '%05%' THEN TRUE ELSE FALSE END AS IS_HIGH_BLOOD_PRESSURE,
	IS_CONTINUE_TAKE_DRUG,
	CASE WHEN ALL_SURGERES LIKE '%01%' THEN TRUE ELSE FALSE END AS ALL_SURGERES_01,
	CASE WHEN ALL_SURGERES LIKE '%02%' THEN TRUE ELSE FALSE END AS ALL_SURGERES_02,
	CASE WHEN ALL_SURGERES LIKE '%03%' THEN TRUE ELSE FALSE END AS ALL_SURGERES_03,
	COALESCE(OTHER_SURGERY,'') AS OTHER_SURGERY,
	COALESCE(REPLACE(REPLACE(REPLACE(COUNT_SURGERY_TYPE,'01','1 ครั้ง'),'02','2 ครั้ง'),'03','มากกว่า 3 ครั้ง'),'') AS COUNT_SURGERY_TYPE,
	COALESCE(SURGERY_CLINIC_NAME,'') AS SURGERY_CLINIC_NAME,
	IS_OPEN_NOSE,
	COALESCE(PROBLEM,'') AS PROBLEM,
	COALESCE(USED_INJECT_NAME,'') AS USED_INJECT_NAME,
	COALESCE(REPLACE(REPLACE(REPLACE(LATEST_INJECT_TIME,'01','น้อยกว่า 1 ปี'),'02','1-3 ปี'),'03','นานกว่า 3 ปี'),'') AS LATEST_INJECT_TIME,
	COALESCE(DRUG_ALLERGY_NAME,'') AS DRUG_ALLERGY_NAME
FROM BOOKING B
LEFT JOIN QUESTIONNAIRE Q
ON (B.QUESTIONNAIRE_ID = Q.ID)
WHERE B.ID = $P{bookingId}]]>
	</queryString>
