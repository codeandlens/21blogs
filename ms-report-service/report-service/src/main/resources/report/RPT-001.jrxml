	<parameter name="dateTo" class="java.util.Date"/>
	<parameter name="dateFromTh" class="java.lang.String"/>
	<parameter name="dateToTh" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT BK.DOCTOR_NAME_TH,
	BK.BRANCH_NAME,
	APT.APPOINTMENT_TYPE_NAME_TH,
	COUNT(BK.ID) AS BOOKING_AMOUNT
FROM BOOKING BK
	INNER JOIN APPOINTMENT APT ON BK.APPOINTMENT_TYPE = APT.APPOINTMENT_TYPE
	INNER JOIN DOCTOR DR ON BK.DOCTOR_ID = DR.DOCTOR_SEQ
WHERE BOOKING_DATE >=  COALESCE($P{dateFrom},BOOKING_DATE)
  AND BOOKING_DATE <= COALESCE($P{dateTo},BOOKING_DATE)
GROUP BY BK.DOCTOR_NAME_TH,BK.BRANCH_NAME,APT.APPOINTMENT_TYPE
ORDER BY DR.DOCTOR_SEQ,BK.BRANCH_ID;]]>
	</queryString>
